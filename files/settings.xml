<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

	<servers>
		<server>
			<id>multiposter-releases</id>
			<username>${env.RELEASE_REPO_USERNAME}</username>
			<password>${env.RELEASE_REPO_PASSWORD}</password>
		</server>
		<server>
			<id>multiposter-snapshots</id>
			<username>${env.SNAPSHOT_REPO_USERNAME}</username>
			<password>${env.SNAPSHOT_REPO_PASSWORD}</password>
		</server>
	</servers>

	<profiles>
		<profile>
			<id>artifactory</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<repositories>
				<repository>
					<id>multiposter-releases</id>
					<name>multiposter-releases</name>
					<url>${env.RELEASE_MULTIPOSTER_URL}</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>multiposter-snapshots</id>
					<name>multiposter-snapshots</name>
					<url>${env.SNAPSHOT_MULTIPOSTER_URL}</url>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
			</repositories>
		</profile>
	</profiles>
</settings>
